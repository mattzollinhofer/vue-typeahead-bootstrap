{
  "name": "vue-typeahead-bootstrap",
  "version": "2.6.0",
  "private": false,
  "description": "A typeahead/autocomplete component for Vue 2 using Bootstrap 4",
  "keywords": [
    "vue",
    "autocomplete",
    "bootstrap",
    "bootstrap 4",
    "omnisearch",
    "typeahead"
  ],
  "homepage": "https://github.com/mattzollinhofer/vue-typeahead-bootstrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattzollinhofer/vue-typeahead-bootstrap"
  },
  "bugs": {
    "url": "https://github.com/mattzollinhofer/vue-typeahead-bootstrap/issues"
  },
  "license": "MIT",
  "author": "Matt Zollinhofer <vue.typeahead.bootstrap@gmail.com>",
  "main": "src/components/VueTypeaheadBootstrap.vue",
  "module": "dist/VueTypeaheadBootstrap.esm.js",
  "unpkg": "dist/VueTypeaheadBootstrap.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name VueTypeaheadBootstrap src/components/VueTypeaheadBootstrap.vue",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/VueTypeaheadBootstrap.min.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./deploy.sh",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:unit:watch": "vue-cli-service test:unit --watch"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-unit-jest": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-standard": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "coveralls": "^3.0.2",
    "highlight.js": "^9.12.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.0.1",
    "rollup-plugin-vue": "^5.1.9",
    "underscore": "^1.9.1",
    "vue-gtm": "^2.0.0",
    "vue-hljs": "^1.0.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "whatwg-fetch": "^2.0.4"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
